{
	"parser": "vue",
	"arrowParens": "always",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"endOfLine": "lf",
	"semi": true,
	"singleQuote": false,
	"tabWidth": 2,
	"trailingComma": "all",
	"useTabs": false,
	"vueIndentScriptAndStyle": false,
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"plugins": ["prettier-plugin-tailwindcss"]
}
